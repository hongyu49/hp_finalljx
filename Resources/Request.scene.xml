<?xml version="1.0" encoding="UTF-8"?>
<scene xmlns="http://www.horizonpower.com.cn/hpclient/scene-xml/" xmlns:builtin="http://www.horizonpower.com.cn/hpclient/scene-xml/built-in/">
    <component name="root">
        <componentType>
			<component name="application" type="builtin:appController"/>
            <component name="main" type="builtin:webView">
                <frame x="0" y="0" width="320" height="300"/>
               
            </component>
            <component name="approveButton" type="builtin:button">
                <frame x="30" y="310" width = "100" height="50"/>
                <properties>
                    <property name="text">
                        <string>同意</string>
                    </property>
                </properties>
            </component>
            <component name="rejectButton" type="builtin:button">
                <frame x="190" y="310" width = "100" height="50"/>
                <properties>
                    <property name="text">
                        <string>驳回</string>
                    </property>
                </properties>
            </component>
        </componentType>
    </component>
</scene>
