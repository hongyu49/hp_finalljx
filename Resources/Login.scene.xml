					<property name="text">
						<string>记住用户信息</string>
					</property>
				</properties>
			</component>
			<component name="cookieSwitch" type="builtin:switch">
                <frame x="210" y="260" width = "79" height="27"/>
                <properties>
                    <property name="on">
                        <string>0</string>
                    </property>
                </properties>
            </component>
			<component name="loginButton" type="builtin:button">
				<frame x="98" y="350" width="150" height="40"/>
				<properties>
					<property name="text">
						<string>登录</string>
					</property>
				</properties>
			</component>
			
        </componentType>
    </component>
</scene>
