<?xml version="1.0" encoding="UTF-8"?>
<scene xmlns="http://www.horizonpower.com.cn/hpclient/scene-xml/" xmlns:builtin="http://www.horizonpower.com.cn/hpclient/scene-xml/built-in/">
<size width="320" height="480"/>
    <component name="root">
		<properties>
			<property name="background">
				<string>http://mobile.sugon.com/view/png/digi/bg_empty.png</string>
			</property>
		</properties>
		<componentType>
			<component name="application" type="builtin:appController"/>
			<component name="case" type="builtin:caseController">
				<properties>
					<property name="title">
						<string>zhuxiao</string>
